{
  "name": "@fragno-example/stripe",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run --passWithNoTests",
    "types:check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:fragno:generate": "fragno-cli db generate src/lib/stripe.ts -o src/db/subscriptions.schema.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun src/db/seed-users.ts",
    "auth:generate": "bunx @better-auth/cli generate --config src/lib/auth/auth.ts --output src/db/auth.schema.ts",
    "stripe:webhook": "stripe listen --forward-to localhost:3000/api/stripe/webhook",
    "stripe:sync-products": "bun src/scripts/sync-products-to-stripe.ts"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.3.15",
    "@fragno-dev/db": "workspace:*",
    "@fragno-dev/stripe": "workspace:*",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-devtools": "^0.7.11",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router": "^1.151.6",
    "@tanstack/react-router-devtools": "^1.151.6",
    "@tanstack/react-router-ssr-query": "^1.151.6",
    "@tanstack/react-start": "^1.153.0",
    "@tanstack/router-plugin": "^1.151.6",
    "better-auth": "^1.4.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.44.7",
    "kysely-pglite": "^0.6.1",
    "lucide-react": "^0.546.0",
    "react": "^19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@fragno-dev/cli": "workspace:*",
    "@fragno-private/typescript-config": "workspace:*",
    "@fragno-private/vitest-config": "workspace:*",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^22.19.7",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-istanbul": "^3.2.4",
    "dotenv": "^17.2.3",
    "jsdom": "^27.4.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
