{
  "name": "@fragno-example/stripe",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run --passWithNoTests",
    "types:check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "fragno:generate": "fragno-cli db generate src/lib/stripe.ts -o src/db/subscriptions.schema.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun src/db/seed-users.ts",
    "auth:generate": "bunx @better-auth/cli generate --config src/lib/auth/auth.ts --output src/db/auth.schema.ts",
    "stripe:webhook": "stripe listen --forward-to localhost:3000/api/stripe/webhook",
    "stripe:sync-products": "bun src/scripts/sync-products-to-stripe.ts"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.3.11",
    "@fragno-dev/db": "workspace:*",
    "@fragno-dev/stripe": "workspace:*",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.15",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-query": "^5.66.5",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-router": "^1.139.3",
    "@tanstack/react-router-devtools": "^1.139.3",
    "@tanstack/react-router-ssr-query": "^1.139.3",
    "@tanstack/react-start": "^1.139.3",
    "@tanstack/router-plugin": "^1.139.3",
    "better-auth": "^1.3.28",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "lucide-react": "^0.546.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.1.15",
    "tw-animate-css": "^1.3.6",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@fragno-dev/cli": "workspace:*",
    "@fragno-private/typescript-config": "workspace:*",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.18.12",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "dotenv": "^17.2.3",
    "jsdom": "^27.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.11",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0"
  }
}
