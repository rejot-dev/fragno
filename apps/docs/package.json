{
  "name": "@fragno-dev/docs-rr",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "deploy": "pnpm run build && wrangler deploy",
    "dev": "react-router dev",
    "postinstall": "fumadocs-mdx",
    "preview": "pnpm run build && vite preview",
    "types:check": "tsc -b --noEmit",
    "types:generate": "wrangler types --env-interface CloudflareEnv && react-router typegen && fumadocs-mdx",
    "db:fragno:generate": "fragno-cli db generate app/fragno/mailing-list.ts -o app/db/mailing-list-do.schema.ts && fragno-cli db generate app/fragno/forms.ts -o app/db/forms-do.schema.ts",
    "db:mailing-list-do:generate": "drizzle-kit generate --config mailing-list-do.drizzle.config.ts",
    "db:forms-do:generate": "drizzle-kit generate --config forms-do.drizzle.config.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@fragno-dev/db": "workspace:*",
    "@fragno-dev/forms": "workspace:*",
    "@fragno-dev/fragment-mailing-list": "workspace:*",
    "@fragno-dev/jsonforms-shadcn-renderers": "workspace:*",
    "@jsonforms/core": "^3.7.0",
    "@jsonforms/react": "^3.7.0",
    "@marsidev/react-turnstile": "^1.3.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-router/node": "^7.9.5",
    "@react-router/serve": "^7.9.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.7",
    "embla-carousel-react": "^8.6.0",
    "fumadocs-core": "16.1.0",
    "fumadocs-mdx": "14.0.3",
    "fumadocs-ui": "16.1.0",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "isbot": "^5.1.32",
    "lucide-react": "^0.555.0",
    "motion": "^12.23.24",
    "react": "^19.2.0",
    "react-day-picker": "^9.12.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.5",
    "shiki": "^3.15.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.5",
    "@fragno-private/vitest-config": "workspace:*",
    "@react-router/dev": "^7.9.5",
    "@tailwindcss/vite": "^4.1.16",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitest/coverage-istanbul": "^3.2.4",
    "drizzle-kit": "^0.31.7",
    "react-router-devtools": "^5.1.3",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite": "^7.2.0",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.51.0"
  }
}
