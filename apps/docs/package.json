{
  "name": "@fragno-dev/docs",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "build": "fumadocs-mdx && next build",
    "dev": "next dev --turbo --port 3003",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "types:check": "tsc --noEmit",
    "types:generate": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "cf-typegen": "pnpm run types:generate"
  },
  "dependencies": {
    "@fragno-dev/core": "workspace:*",
    "@opennextjs/cloudflare": "^1.9.1",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "fumadocs-core": "15.8.2",
    "fumadocs-mdx": "12.0.1",
    "fumadocs-typescript": "^4.0.10",
    "fumadocs-ui": "15.8.2",
    "lucide-react": "^0.544.0",
    "motion": "^12.23.22",
    "next": "15.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "shiki": "^3.13.0",
    "tailwind-merge": "^3.3.1",
    "ts-morph": "^27.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@opennextjs/aws": "^3.8.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/mdx": "^2.0.13",
    "@types/node": "24.5.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "wrangler": "^4.40.3"
  }
}
